module Boost3v7To5v:
    power_in = new Power
    power_out = new Power
    ic = new TPS61022RWUR
    cin = new Capacitor
    rfbt = new Resistor
    rfbb = new Resistor
    cout = new Capacitor
    l1 = new Inductor
    # https://www.digikey.com/en/products/detail/coilcraft/XAL4020-102MEC/21381951
    l1.mpn = "XAL4020-102MEC"
    l1.footprint = "L1616"
    cin.footprint = "C0805"
    cin.value = 10uF +/- 20%
    cin.voltage = 10V +/- 20%
    cin.esr = 3mohm
    rfbb.value = 100kohm +/- 1%
    rfbb.footprint = "R0603"
    cout.footprint = "C1206"
    cout.value = 47uF +/- 20%
    cout.voltage = 10V +/- 20%
    cout.esr = 3.71mohm
    rfbt.value = 732kohm +/- 1%
    rfbt.footprint = "R0603"
    l1.value = 1uH +/- 20%
    l1.dcr = 13.3mohm
    l1.idc = 8.7A
    signal gnd
    power_in.gnd ~ gnd
    power_out.gnd ~ gnd
    power_out.vcc ~ cout.p1
    power_in.vcc ~ ic.vin
    power_in.vcc ~ cin.p1
    power_in.vcc ~ l1.p1
    power_in.vcc ~ ic.en
    gnd ~ cin.p2
    gnd ~ ic.mode
    gnd ~ ic.gnd
    gnd ~ rfbb.p2
    gnd ~ cout.p2
    ic.sw ~ l1.p2
    ic.vout ~ rfbt.p1
    ic.vout ~ cout.p1
    ic.vout ~ power_out.vcc
    ic.fb ~ rfbt.p2
    ic.fb ~ rfbb.p1

