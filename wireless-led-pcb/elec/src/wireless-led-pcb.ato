module QualLevelShifter:
    ic = new SN74AHC125QPWRQ1
    power = new Power
    signal in1 ~ ic.in1
    signal in2 ~ ic.in2
    signal in3 ~ ic.in3
    signal in4 ~ ic.in4
    signal out1 ~ ic.out1
    signal out2 ~ ic.out2
    signal out3 ~ ic.out3
    signal out4 ~ ic.out4
    ic.oe1 ~ power.gnd
    ic.oe2 ~ power.gnd
    ic.oe3 ~ power.gnd
    ic.oe4 ~ power.gnd

