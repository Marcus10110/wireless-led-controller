module Ldo5V:
    power_in = new Power
    power_out = new Power

    ic = new AP7375_50SA_7
    cin = new Capacitor
    cout = new Capacitor

    signal gnd
    power_in.vcc ~ ic.v_in
    power_out.vcc ~ ic.v_out
    gnd ~ ic.gnd
    power_in.gnd ~ gnd
    power_out.gnd ~ gnd
    power_out.vcc ~ cout.p1
    power_in.vcc ~ cin.p1
    gnd ~ cin.p2
    gnd ~ cout.p2
    cin.footprint = "C0603"
